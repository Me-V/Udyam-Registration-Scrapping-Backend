generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Registration {
  id            Int      @id @default(autoincrement())
  aadhaarNumber String   @unique
  panNumber     String   @unique
  businessName  String
  businessType  String
  createdAt     DateTime @default(now())
}